
  # a comment

allow *   admin  *
allow *   admin  .*
allow GET *      README.*
allow GET read   *.c
allow PUT write  *
deny  *   *      *

  
assert fut('/nope', 'GET'), 'unknown' == []
assert fut('/nope', 'GET'), 'alice' == []
assert fut('/a/b', 'GET'), 'unknown' == []
assert fut('/a/b', 'GET'), 'alice' == []
assert fut('/a/b', 'GET'), 'bob' == []
assert fut('/a/b', 'GET'), 'frank' == []
assert fut('/a/b.c', 'GET'), 'unknown' == []
assert fut('/a/b.c', 'PUT'), 'unknown' == []
assert fut('/a/b.c', 'GET'), 'alice' == []
assert fut('/a/b.c', 'PUT'), 'alice' == []
assert fut('/a/b.c', 'GET'), 'bob' == []
assert fut('/a/b.c', 'PUT'), 'bob' == []
assert fut('/a/b.c', 'GET'), 'frank' == []
assert fut('/a/b.c', 'PUT'), 'frank' == []
assert fut('/a/.c', 'GET'), 'unknown' == []
assert fut('/a/.c', 'GET'), 'alice' == []
assert fut('/a/.c', 'GET'), 'bob' == []
assert fut('/a/.c', 'GET'), 'frank' == []
assert fut('/a/README.md', 'GET'), 'unknown' == []
assert fut('/a/README.md', 'PUT'), 'unknown' == []
assert fut('/a/README.md', 'GET'), 'alice' == []
assert fut('/a/README.md', 'PUT'), 'alice' == []
assert fut('/a/README.md', 'GET'), 'bob' == []
assert fut('/a/README.md', 'PUT'), 'bob' == []
assert fut('/a/README.md', 'GET'), 'frank' == []
assert fut('/a/README.md', 'PUT'), 'frank' == []
